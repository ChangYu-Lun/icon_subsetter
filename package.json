{
  "name": "icon_subsetter",
  "version": "1.0.0",
  "description": "Material Symbols font subsetter tool",
  "main": "index.js",
  "scripts": {
    "build:font:rounded": "node scripts/icon-subsetter.js build --type=rounded",
    "build:font:outlined": "node scripts/icon-subsetter.js build --type=outlined",
    "build:font:sharp": "node scripts/icon-subsetter.js build --type=sharp",
    "build:font:all": "npm run build:font:rounded && npm run build:font:outlined && npm run build:font:sharp",
    "font:compare:rounded": "node scripts/icon-subsetter.js --compare --type=rounded",
    "font:compare:outlined": "node scripts/icon-subsetter.js --compare --type=outlined",
    "font:compare:sharp": "node scripts/icon-subsetter.js --compare --type=sharp",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "material-symbols",
    "font",
    "subset",
    "optimization"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "glob": "^10.3.10"
  }
}